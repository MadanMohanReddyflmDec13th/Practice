package jan30th;

public class MethodsDemo1 {

	public static void main(String[] args) {
		
		//Sum of n natural numbers
		int n=5;
		int res=(n*(n+1)/2);
		System.out.println("Sum of "+n+" natural numbers is "+res);
		
		int n2=10;
		int res2=(n2*(n2+1)/2);
		System.out.println("Sum of "+n2+" natural numbers is "+res2);

				
			}
	}

}
