package programs;

public class Swap2Nums {
	public static void main(String[] args) {
		
	Swap2Nums obj=new Swap2Nums() ;
	obj.swapp();
		
	}
		//using 3rd variable
		int a=10;
		int b=25;
		int temp;
		
		public int swapp() {
	
		temp=a;
		a=b;
		b=temp;
		return a;
		}	
		//System.out.println(a);
		//System.out.println(b);
		
		//without 3rd variable
		
		
	

}
